@article{dar2022ReducingRequirementsAmbiguity,
  title = {Reducing {{Requirements Ambiguity}} via {{Gamification}}: {{Comparison}} with {{Traditional Techniques}}},
  shorttitle = {Reducing {{Requirements Ambiguity}} via {{Gamification}}},
  author = {Dar, Hafsa Shareef and Imtiaz, Salma and Lali, Muhammad IkramUllah},
  editor = {Zhang, Dalin},
