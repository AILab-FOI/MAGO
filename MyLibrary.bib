@article{SILVEIRA2016,
  title = {Towards a {{Model}} of {{Open}} and {{Reliable Cognitive Multiagent Systems}}: {{Dealing}} with {{Trust}} and {{Emotions}}},
  author = {Silveira, Ricardo and Klein da Silva Bitencourt, Guilherme and Gelaim, Thiago \^Angelo and Marchi, Jerusa and family=Prieta, given=Fernando, prefix=de la, useprefix=true},
