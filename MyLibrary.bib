  shorttitle = {Tailored {{Gamification}} in {{Education}}},
  author = {Oliveira, Wilk and Hamari, Juho and Shi, Lei and Toda, Armando M. and Rodrigues, Luiz and Palomino, Paula T. and Isotani, Seiji},
  date = {2023},
  journaltitle = {Education and Information Technologies},
  shortjournal = {Educ Inf Technol},
  volume = {28},
