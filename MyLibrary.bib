  author = {Riveret, R\'egis and Gao, Yang and Governatori, Guido and Rotolo, Antonino and Pitt, Jeremy and Sartor, Giovanni},
  date = {2019-03},
  journaltitle = {Autonomous Agents and Multi-Agent Systems},
  shortjournal = {Auton Agent Multi-Agent Syst},
