@online{giglouLLMs4OLLargeLanguage2023,
  title = {{{LLMs4OL}}: {{Large Language Models}} for {{Ontology Learning}}},
  shorttitle = {{{LLMs4OL}}},
  author = {Giglou, Hamed Babaei and D'Souza, Jennifer and Auer, S\"oren},
