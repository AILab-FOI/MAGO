@article{yaacoub2023SecurityFederatedLearning,
  title = {Security of {{Federated Learning}} with {{IoT Systems}}: {{Issues}}, {{Limitations}}, {{Challenges}}, and {{Solutions}}},
  shorttitle = {Security of {{Federated Learning}} with {{Iot Systems}}},
  author = {Yaacoub, Jean-Paul A. and Noura, Hassan N. and Salman, Ola},
