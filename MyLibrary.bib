@article{dereli2024GamificationPhysiotherapyRehabilitation,
  title = {Gamification in Physiotherapy and Rehabilitation Education: A Narrative Review},
  shorttitle = {Gamification in Physiotherapy and Rehabilitation Education},
  author = {Dereli, Muge and Kahraman, Turhan},
  date = {2024-02-14},
  journaltitle = {Bulletin of Faculty of Physical Therapy},
  shortjournal = {Bull Fac Phys Ther},
