@article{deweerd2017NegotiatingOtherMinds,
  title = {Negotiating with Other Minds: The Role of Recursive Theory of Mind in Negotiation with Incomplete Information},
  shorttitle = {Negotiating with Other Minds},
  author = {family=Weerd, given=Harmen, prefix=de, useprefix=true and Verbrugge, Rineke and Verheij, Bart},
  date = {2017-03},
  journaltitle = {Autonomous Agents and Multi-Agent Systems},
