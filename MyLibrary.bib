@article{fadzliRealTime3DReconstruction2022,
  title = {Real-{{Time 3D Reconstruction Method}} for {{Holographic Telepresence}}},
  author = {Fadzli, Fazliaty Edora and Ismail, Ajune Wanis and Abd Karim Ishigaki, Shafina and Nor'a, Muhammad Nur Affendy and Aladin, Mohamad Yahya Fekri},
