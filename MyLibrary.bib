@incollection{igarashi2017MulticriteriaCoalitionFormation,
  title = {Multi-Criteria {{Coalition Formation Games}}},
  booktitle = {Algorithmic {{Decision Theory}}},
  author = {Igarashi, Ayumi and Roijers, Diederik M.},
  editor = {Rothe, J\"org},
  date = {2017},
  volume = {10576},
