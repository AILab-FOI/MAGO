@incollection{toda2018DarkSideGamification,
  title = {The {{Dark Side}} of {{Gamification}}: {{An Overview}} of {{Negative Effects}} of {{Gamification}} in {{Education}}},
  shorttitle = {The {{Dark Side}} of {{Gamification}}},
  booktitle = {Higher {{Education}} for {{All}}. {{From Challenges}} to {{Novel Technology-Enhanced Solutions}}},
  author = {Toda, Armando M. and Valle, Pedro H. D. and Isotani, Seiji},
  editor = {Cristea, Alexandra Ioana and Bittencourt, Ig Ibert and Lima, Fernanda},
  date = {2018},
  volume = {832},
  pages = {143--156},
  publisher = {Springer International Publishing},
  location = {Cham},
  doi = {10.1007/978-3-319-97934-2_9},
  url = {http://link.springer.com/10.1007/978-3-319-97934-2_9},
  urldate = {2021-05-03},
  abstract = {Gamification has a great number of studies in the education area since the emergence of the term. However, there is a lack of primary and secondary studies that explore the negative effects that gamification may have on learners, and lack of studies that analyze the gamification design that are linked to those negative effects. Based on this premise, we aim at answering the following research question ``What are the negative effects that can occur in gamification when applied to educational contexts?''. We seek to answer this question by analyzing the negative effects that are associated to gamification and the gamified learning design that are linked with them. To answer this question, we conducted a systematic mapping study to identify these negative effects. Based on the studies that were analyzed, we identified and mapped 4 negative effects and their gameful design. Loss of Performance was the most occurring effect and Leaderboard the most cited game design element, among other 11 elements. Moreover, elements and effects were linked in order to identify how these elements may have influenced on these outcomes. Based on our results, we found out that the game design may lead to a negative impact. For instance, Leaderboards are strongly associated to many negative effects mapped in this work. This result is corroborated by the psychology literature regarding ranking systems within learning environments. We believe our work may be useful to guide gamification instructors and specialists to avoid those negative effects in education contexts, by avoiding some game design elements settings.},
  isbn = {978-3-319-97933-5 978-3-319-97934-2},
  langid = {english},
  file = {/home/bogdan/Documents/Literatura/Bib/Toda, Valle, Isotani - 2018 - The Dark Side of Gamification - An Overview of Negative Effects of Gamification.pdf}
}

