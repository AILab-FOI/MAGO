  pagetotal = {160},
  keywords = {Academic writing,writing},
  file = {/home/bogdan/Documents/Literatura/Bib/Turabian - 1996 - A Manual for Writers of Research Papers, Theses, and Dissertations.mobi;/home/bogdan/Documents/Literatura/Bib/Turabian - 1996 - A Manual for Writers of Research Papers, Theses, and Dissertations.pdf}
