@article{li2023CognitiveMotivationalEngagementICT,
  title = {Cognitive-{{Motivational Engagement}} in {{ICT Mediates}} the {{Effect}} of {{ICT Use}} on {{Academic Achievements}}: {{Evidence}} from 52 {{Countries}}},
  shorttitle = {Cognitive-{{Motivational Engagement}} in {{Ict Mediates}} the {{Effect}} of {{Ict Use}} on {{Academic Achievements}}},
  author = {Li, Sandy C. and Zhu, Jinxin},
  date = {2023-10},
  journaltitle = {Computers \& Education},
  shortjournal = {Computers \& Education},
  volume = {204},
  pages = {104871},
  issn = {03601315},
  doi = {10.1016/j.compedu.2023.104871},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0360131523001483},
  urldate = {2024-05-27},
  abstract = {Despite huge investment of information and communication technology (ICT) in schools across the world, recent studies showed its non-positive association with academic achievement. While educators or policymakers may doubt the contribution of ICT in education, few studies examined the indirect effect of ICT use on academic achievement via ICT-related dispositions, such as students' perceived autonomy, competence, and interest in ICT (cognitive-motivational engagement in ICT). Guided by self-determination theory (SDT) and related literature, we hypothesized a three-level structural equation model to examine the interplay between ICT use, cognitivemotivational engagement in ICT, and academic achievement based on 15-year-old secondary students from 52 countries in the PISA 2018. The results showed cognitive-motivational engagement in ICT exerted a positive mediating effect between ICT use and academic achieve\- ment at the student and school levels, indicating ICT use exerted an indirect positive effect on academic achievement, and satisfaction of students' basic psychological needs in a specific area such as ICT is likely to promote their optimal functioning in other areas such as learning. Hence, students should be provided more opportunities to orchestrate technologies in formal and informal contexts helps develop their self-determination, discernment, and self-regulation in ICT use, and thereby creating the conditions in which better learning outcomes are more likely to occur. These findings also deepen our understanding of SDT and provide a guiding framework for educators to design pedagogies that can better address students' ICT-related psychological needs. Finally, this study shows that a quadratic term should be modeled when examining the rela\- tionship between ICT use and academic achievement.},
  langid = {english},
  file = {/home/bogdan/Documents/Literatura/Bib/Li, Zhu - 2023 - Cognitive-Motivational Engagement in ICT Mediates the Effect of ICT Use on.pdf}
}

@article{li2023DiversifiedResourceAccess,
  title = {Diversified {{Resource Access Paths}} in {{MOOCs}}: {{Insights}} from {{Network Analysis}}},
  shorttitle = {Diversified {{Resource Access Paths}} in {{Moocs}}},
  author = {Li, Shuang and Du, Junlei and Yu, Shu},
  date = {2023-10},
  journaltitle = {Computers \& Education},
  shortjournal = {Computers \& Education},
  volume = {204},
  pages = {104869},
  issn = {03601315},
  doi = {10.1016/j.compedu.2023.104869},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S036013152300146X},
  urldate = {2024-05-27},
  abstract = {The low access rate to resources and learner diversity of massive open online courses (MOOCs) has led to a shift in MOOC resource design toward how to better support adaptive learning through personalized recommendation, highlighting the importance of exploring diversified ac\- cess paths for MOOC resources and their dynamics. Nevertheless, most studies have explored the MOOC learning process from the perspective of behavior sequences or activity distributions, thus limiting the ability to reveal the diversified paths learners use to access MOOC resources. From a perspective of network analysis, this study used log data generated by 12,468 participants from a Chinese MOOC to build resource access networks of learners with resources as nodes. This data was then used to investigate learners' diversified paths to access content-related resources by examining the structure and dynamics of the networks. Learners with different engagement and achievement levels differ in chapter access paths, specific resource access paths, and the dynamic characteristics of their access behavior patterns. Compared with the scattered, localized, and short access paths of the subpopulations not taking the exam, the final exam-taker subpopulations showed coherent, expanded and long access paths across all chapters and tended to use quizzes as scaffolding for other resources within and across chapters. Notably, successful learners showed a trend of reciprocity in resource access early in the course, an important feature that distinguished them from the other subpopulations. The findings inform the design of personalized resource recommendation systems for adaptive learning in MOOCs.},
  langid = {english},
  file = {/home/bogdan/Documents/Literatura/Bib/Li, Du, Yu - 2023 - Diversified Resource Access Paths in MOOCs.pdf}
}

@article{liemhetcharat2017AllocatingTrainingInstances,
  title = {Allocating Training Instances to Learning Agents for Team Formation},
  author = {Liemhetcharat, Somchaya and Veloso, Manuela},
