@article{Muhanna1994,
  title = {Meta-Modeling Concepts and Tools for Model Management: A Systems Approach},
  author = {family=Muhanna, given=WA, given-i=WA and family=Pick, given=RA, given-i=RA},
  date = {1994},
  journaltitle = {Management Science},
  volume = {40},
