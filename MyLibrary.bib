@article{haanpaa2022SerializedGameStructure,
  title = {Serialized {{Game Structure}} and {{Serialized Paratexts}}: {{Engaging Players}} in {{Apex Legends}}},
  shorttitle = {Serialized {{Game Structure}} and {{Serialized Paratexts}}},
  author = {Haanp\"a\"a, Juuso},
  date = {2022},
  url = {https://trepo.tuni.fi/handle/10024/140854},
  urldate = {2024-10-15},
  abstract = {The topic of this thesis is how serialization and paratextuality are used to engage players in Apex Legends, a multiplayer video game. Apex Legends is a multiplayer video game that has retained a large player base since its launch in 2019. This thesis argues that Apex Legends retains its player base because the game's serialization and paratexts keep players engaged with the game.  To study how Apex Legends engages its players, the thesis uses a framework that allows for the analysis of both the game's in-game seriality and the seriality of the game's paratexts. The theory section of the thesis also explains how seriality can be analyzed in video games without sequels. In addition, the thesis clarifies how the paratexts of Apex Legends are a part of a player's experience with the game. Apex Legends has received many updates, trailers, and user generated pieces of content during the time it has been playable. The thesis analyzes these different types of content to determine how they keep players returning to the game.  The analysis section of the thesis is divided into two sections. The first section is focused on how Apex Legends is serialized as a video game; this section examines how the game's in-game structure and narrative are serialized. The game's serialization into seasons that keep players engaged and playing the game is also discussed. The second section of the analysis focuses on the paratexts of Apex Legends and their serialization: the thesis argues that the trailers and animated shorts set in the world of the game work together with the game to create serial narratives. In addition, the second section analyzes how the paratexts produced by users of Apex Legends engage players with the game by keeping them interested in the game when they are not playing it.},
  langid = {english},
  annotation = {Accepted: 2022-06-09T13:37:36Z},
  file = {/home/bogdan/Zotero/storage/VEZJ7XRS/140854.html}
}

