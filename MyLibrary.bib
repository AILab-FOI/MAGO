@article{shpakova2017ChangingGameCase,
  title = {Changing the {{Game}}: {{A Case}} for {{Gamifying Knowledge Management}}},
  shorttitle = {Changing the {{Game}}},
  author = {Shpakova, Agnessa and D\"orfler, Viktor and MacBryde, Jill},
